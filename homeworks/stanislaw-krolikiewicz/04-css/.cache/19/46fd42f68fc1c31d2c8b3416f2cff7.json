E64AA;\n  --clr-gray-1: #3D3D3D;\n  --clr-gray-2: #575757;\n  --clr-light-gray-1: #A3ABB2;\n  --clr-light-gray-2: #D7D7D7;\n  --clr-dark-blue-1: #0C151D;\n  --clr-dark-blue-2: #171F26;\n  --clr-yellow-1: #FBD144;\n  --clr-yellow-2: #FFE071;\n}\n\n:root.light {\n\n  --clr-page-bg: var(--clr-light-1);\n\n  --clr-yellow: var(--clr-yellow-1);\n\n  --clr-primary-text: var(--clr-gray-1);\n\n  --clr-secondary-text: var(--clr-gray-2);\n\n  --clr-primary-button-bg: var(--clr-yellow);\n  --clr-primary-button-text: var(--clr-gray-1);\n  --clr-primary-button-bg--hover: var(--clr-blue);\n  --clr-primary-button-text--hover: var(--clr-white);\n  //      secondary\n  --clr-secondary-button-bg: var(--clr-white);\n  --clr-secondary-button-text: var(--clr-gray-1);\n  --clr-secondary-button-bg--hover: transparent;\n  --clr-sendaary-button-border--hover: var(--clr-blue);\n  --clr-secondary-button-text--hover: var(--clr-blue);\n\n    // TABS\n    --clr-tab-bg: var(--clr-gray);\n    --clr-tab-text: var(--clr-dark);\n}\n\n:root.dark {\n  // page\n  --clr-page-bg: var(--clr-dark-blue-1);\n\n  // GENERAL\n  --clr-yellow: var(--clr-yellow-2);\n\n  // TEXT\n  //      primary\n  --clr-primary-text: var(--clr-light-2);\n  //      secondary\n  --clr-secondary-text: var(--clr-light-gray);\n\n  // BUTTONS\n  //      primary\n  --clr-primary-button-bg: var(--clr-yellow);\n  --clr-primary-button-text: var(--clr-gray);\n  --clr-primary-button-bg--hover: var(--clr-blue);\n  --clr-primary-button-text--hover: var(--clr-primary-text);\n  //      secondary\n  --clr-secondary-button-bg: var(--color-dark-blue-2);\n  --clr-secondary-button-text: var(--clr-secondary-text);\n  --clr-secondary-button-bg--hover: transparent;\n  --clr-sendaary-button-border--hover: var(--clr-blue);\n  --clr-secondary-button-text--hover: var(--clr-blue);\n\n  // TABS\n  --clr-tab-bg: var(--clr-dark-blue);\n  --clr-tab-text: var(--clr-light-gray);\n}","styles/scss/_themeSwitcher.scss":"@import 'variables';\n\n$clr-bg--light: var(--clr-white);\n$clr-fill--light: var(--clr-gray-1);\n$clr-bg--dark: var(--clr-dark-blue-2);\n$clr-fill--dark: var(--clr-light-gray-1);\n$clr-border-light: var(--clr-light-3);\n$clr-border-dark: var(--clr-dark-blue-1);\n$size: 2.5rem;\n$padding: 1.25rem;\n\n.theme-switcher {\n  width: calc($size + 2 * $padding);\n  height: calc($size + 2 * $padding);\n  border: none;\n  background-color: transparent;\n  border-radius: 50%;\n  overflow: hidden;\n  &:hover {\n    cursor: pointer;\n  }\n  @at-root &__icon {\n    width: 100%;\n    height: 100%;\n    transition: all 0.3s ease-in-out;\n    border: 1px solid;\n    border-radius: 50%;\n    padding: calc($padding - 2px);\n    background-color: transparent;\n    @at-root &--light {\n      background-color: $clr-bg--light;\n      fill: $clr-fill--light;\n      border-color: $clr-border-light;\n      &:hover {\n        background-color: $clr-bg--dark;\n        fill: $clr-fill--dark;\n        border-color: $clr-border-dark;\n      }\n    }\n    @at-root &--dark {\n      background-color: $clr-bg--dark;\n      fill: $clr-fill--dark;\n      border-color: $clr-border-dark;\n      &:hover {\n        background-color: $clr-bg--light;\n        fill: $clr-fill--light;\n        border-color: $clr-border-light;\n      }\n    }\n  }\n}","styles/scss/_main.scss":"@import 'variables';\n@import 'themeSwitcher';\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\n.page {\n  background-color: var(--clr-page-bg);\n  color: var(--clr-primary-text);\n  transition: all 0.3s ease-in-out;\n}\n"},"lineCount":null}},"error":null,"hash":"8f9d617f427e0a63278da706e313bfd0","cacheData":{"env":{}}}